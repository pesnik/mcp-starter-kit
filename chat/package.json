{
  "name": "@mcp-playground/mcp-chat",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "backend:build": "tsc -p tsconfig.backend.json",
    "backend:start": "npm run backend:build && node dist/backend/server.js",
    "backend:dev": "tsc -p tsconfig.backend.json --watch &amp; nodemon dist/backend/server.js",
    "dev:with-backend": "concurrently \"npm run backend:start\" \"vite\"",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  }
}